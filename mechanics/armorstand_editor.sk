import:
	org.bukkit.util.EulerAngle
	
function armorstandEditGui(p: player, e:entity):
	create a gui with chest inventory with 5 rows named {@gui_Name}:
		if {_e}.hasArms() is true:
			set {_button} to {@on_button} named "&fРуки: &aВКЛ"
		else:
			set {_button} to {@off_button} named "&fРуки: &cВЫКЛ"
		make next gui slot with {_button} with lore {@toggle_lore}:
			if {_e}.hasArms() is true:
				{_e}.setArms(false)
				set gui item to {@off_button} named "&fРуки: &cВЫКЛ" with lore {@toggle_lore}
			else:
				{_e}.setArms(true)
				set gui item to {@on_button} named "&fРуки: &aВКЛ" with lore {@toggle_lore}

		if {_e}.isSmall() is true:
			set {_button} to {@on_button} named "&fРазмер: &aМаленький"
		else:
			set {_button} to {@off_button} named "&fРазмер: &cСтандартный"
		make next gui slot with {_button} with lore {@toggle_lore}:
			if {_e}.isSmall() is false:
				{_e}.setSmall(true)
				set gui item to {@on_button} named "&fРазмер: &aМаленький" with lore {@toggle_lore}
			else:
				{_e}.setSmall(false)
				set gui item to {@off_button} named "&fРазмер: &cСтандартный" with lore {@toggle_lore}

		if {_e}.hasBasePlate() is true:
			set {_button} to {@on_button} named "&fПодставка: &aВКЛ"
		else:
			set {_button} to {@off_button} named "&fПодставка: &cВЫКЛ"
		make gui slot 9 with {_button} with lore {@toggle_lore}:
			if {_e}.hasBasePlate() is true:
				{_e}.setBasePlate(false)
				set gui item to {@off_button} named "&fПодставка: &cВЫКЛ" with lore {@toggle_lore}
			else:
				{_e}.setBasePlate(true)
				set gui item to {@on_button} named "&fПодставка &aВКЛ" with lore {@toggle_lore}
					
		make gui slot 10 with {@rotate_button} named "&fВращение по оси: &a%yaw of {_e}%°" with lore {@button_lore}:
			if click type is left mouse button:
				{_e}.setRotation(yaw of {_e}+1,pitch of {_e})
			else if click type is left mouse button with shift:
				{_e}.setRotation(yaw of {_e}+10,pitch of {_e})
			else if click type is right mouse button:
				{_e}.setRotation(yaw of {_e}-1,pitch of {_e})
			else if click type is right mouse button with shift:
				{_e}.setRotation(yaw of {_e}-10,pitch of {_e})
			if yaw of {_e} is greater than 359:
				{_e}.setRotation(yaw of {_e}-360,pitch of {_e})
			else if yaw of {_e} is less than 0:
				{_e}.setRotation(360+yaw of {_e},pitch of {_e})
			set name of gui item to "&fВращение по оси: &a%yaw of {_e}%°"

		set {_head} to {_e}.getHeadPose()
		make gui slot 3 with {@x_head_button} named "&fВращение &eголовы&f по X: &a%{_head}.getX()%" with lore {@button_lore}:
			set {_head} to {_e}.getHeadPose()
			if click type is left mouse button:
				set {_head} to new EulerAngle({_head}.getX()+0.01,{_head}.getY(),{_head}.getZ())
			else if click type is left mouse button with shift:
				set {_head} to new EulerAngle({_head}.getX()+0.1,{_head}.getY(),{_head}.getZ())
			else if click type is right mouse button:
				set {_head} to new EulerAngle({_head}.getX()-0.01,{_head}.getY(),{_head}.getZ())
			else if click type is right mouse button with shift:
				set {_head} to new EulerAngle({_head}.getX()-0.1,{_head}.getY(),{_head}.getZ())
			{_e}.setHeadPose({_head})
			set name of clicked slot to "&fВращение &eголовы&f по X: &a%{_head}.getX()%"
		make gui slot 12 with {@y_head_button} named "&fВращение &eголовы&f по Y: &a%{_head}.getY()%" with lore {@button_lore}:
			set {_head} to {_e}.getHeadPose()
			if click type is left mouse button:
				set {_head} to new EulerAngle({_head}.getX(),{_head}.getY()+0.01,{_head}.getZ())
			else if click type is left mouse button with shift:
